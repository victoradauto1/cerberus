generator client {
  provider = "prisma-client-js"
  output   = "../data"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model tokens {
  id       String @id @map("_id")
  name     String
  symbol   String
  decimals Int
  network  Int
}

model pools {

  id String @id @map("_id")
  token0 String
  token1 String
  symbol String
  symbol0 String
  symbol1 String
  fee Int
  exchange Int
  network Int
  price0 String
  price0Change Float
  last0Update DateTime
  price1 String
  price1Change Float
  last1Update DateTime
}
